{
  "buildCommand": "cd apps/web && npm run build",
  "outputDirectory": "apps/web/build/client",
  "installCommand": "cd apps/web && npm install",
  "framework": "vite",
  "functions": {
    "apps/web/build/server/index.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
